{
  "compilerOptions": {
    "allowUnreachableCode": false,
    "declaration": false,
    "moduleResolution": "node",
    "module": "esnext",
    "target": "es2017",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true
  },
  "exclude": ["node_modules"],
  "typedocOptions": {
    "ignoreCompilerErrors": true,
    "mode": "modules",
    "json": "./.docs/data.json",
    "exclude": ["**/*.spec.ts", "**/*.e2e.ts"]
  }
}
