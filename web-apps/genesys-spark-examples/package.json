  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@stencil/webpack": "0.0.6",
    "@types/copy-webpack-plugin": "^10.1.0",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
